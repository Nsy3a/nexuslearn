{
  "name": "nexuslearn-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Web3.0 AI驱动的学习平台 - 现代化全栈应用",
  "author": "AI Development Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "build": "vue-tsc && vite build",
    "preview": "vite preview --host 0.0.0.0 --port 4173",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "deploy": "npm run build && npm run deploy:vercel",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "start": "npm run preview"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "element-plus": "^2.4.4",
    "@element-plus/icons-vue": "^2.3.1",
    "web3": "^4.6.0",
    "@wagmi/core": "^2.6.5",
    "viem": "^2.5.0",
    "@web3modal/ethereum": "^4.1.1",
    "@web3modal/vue": "^4.1.1",
    "axios": "^1.6.5",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "vue-echarts": "^6.6.1",
    "lucide-vue-next": "^0.303.0",
    "recharts": "^2.10.4",
    "three": "^0.158.0",
    "zustand": "^4.4.7",
    "vue-i18n": "^9.8.0",
    "@vueuse/core": "^10.7.1",
    "prismjs": "^1.29.0",
    "vue-prism-editor": "^2.0.0-alpha.2",
    "marked": "^11.1.1",
    "dompurify": "^3.0.8",
    "crypto-js": "^4.2.0",
    "qrcode": "^1.5.3",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/crypto-js": "^4.2.1",
    "@types/file-saver": "^2.0.7",
    "@types/qrcode": "^1.5.5",
    "@types/prismjs": "^1.26.3",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "vercel": "^33.1.0",
    "vite": "^5.0.11",
    "vitest": "^1.1.3",
    "vue-tsc": "^1.8.27",
    "@types/three": "^0.158.3",
    "@types/dompurify": "^3.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "keywords": [
    "vue3",
    "typescript",
    "web3",
    "ai",
    "learning",
    "education",
    "blockchain",
    "metaverse",
    "nft",
    "smart-contracts",
    "modern-ui",
    "responsive",
    "glassmorphism"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/nexuslearn-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/nexuslearn-frontend/issues"
  },
  "homepage": "https://nexuslearn.app"
}
